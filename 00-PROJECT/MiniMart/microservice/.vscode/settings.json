{
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "interactive",
    "files.exclude": {
        "**/.git": true,
        "**/.vscode": true,
        "**/target": true,
        "**/.DS_Store": true,
        "**/*.iml": true,
        "**/mvnw": true,
        "**/mvnw.cmd": true,
        "**/.gitignore": true,
        "**/.gitattributes": true,
        "**/.mvn": true,
        "**/HELP.md": true,
    },
    "favorites.resources": [
        {
            "filePath": "configuration-server\\src\\main\\resources\\config",
            "group": "Default"
        },
        {
            "filePath": ".vscode",
            "group": "Default"
        }
    ],
    "favorites.sortOrder": "ASC"
}